{
  "expo": {
    "name": "Bago",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bago-logo.png",
    "splash": {
      "image": "./assets/images/bago-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deracali.boltexponativewind",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to send you personalized notifications.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to detect your country and currency",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.deracali.boltexponativewind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bago-logo.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "usesCleartextTraffic": true,
      "notification": {
        "icon": "./assets/images/bago-logo.png",
        "color": "#6366F1",
        "priority": "max",
        "channelId": "default"
      },
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/images/bago-logo.png",
      "color": "#6366F1",
      "iosDisplayInForeground": true
    },
    "permissions": {
      "location": {
        "ios": {
          "description": "Allow the app to access your location to provide personalized services."
        },
        "android": {
          "description": "Allow the app to access your location to provide personalized services."
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/bago-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b11b74ac-d95e-4ea5-8cf7-0bb68eff100e"
      }
    },
    "owner": "deracali"
  }
}
